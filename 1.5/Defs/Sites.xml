<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--pax branch quests-->
  <WorldObjectDef>
    <defName>HVMP_PaxTalks</defName>
    <label>pax negotiations</label>
    <description>A site where the Pax Branch wants you to negotiate for peaceful cooperation with other inhabitants of the planet.</description>
    <worldObjectClass>HautsPermits.WorldObject_PaxTalks</worldObjectClass>
    <texture>World/WorldObjects/PeaceTalks</texture>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/WorldObjects/Expanding/PeaceTalks</expandingIconTexture>
    <expandingIconPriority>70</expandingIconPriority>
    <comps>
      <li Class="WorldObjectCompProperties_Timeout" />
    </comps>
  </WorldObjectDef>
  
  <!--rover branch quests-->
  <WorldObjectDef>
    <defName>HVMP_AtlasPoint</defName>
    <label>atlas revision point</label>
    <description>This region is important to an atlas you have been tasked with revising. In order for someone to be able to work on the atlas, they must visit this place first.</description>
    <worldObjectClass>HautsPermits.WorldObject_AtlasPoint</worldObjectClass>
	<texture>World/WorldObjects/JourneyDestination</texture>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/WorldObjects/Expanding/JourneyDestination</expandingIconTexture>
    <expandingIconPriority>71</expandingIconPriority>
    <comps>
      <li Class="WorldObjectCompProperties_Timeout" />
    </comps>
  </WorldObjectDef>
  <WorldObjectDef>
    <defName>HVMP_OdysseyPoint</defName>
    <label>mapping point</label>
    <description>Visiting this place is essential to establishing a contemporary version of the Rover Branch's regional map.</description>
    <worldObjectClass>HautsPermits.WorldObject_OdysseyPoint</worldObjectClass>
	<texture>World/WorldObjects/JourneyDestination</texture>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/WorldObjects/Expanding/JourneyDestination</expandingIconTexture>
    <expandingIconPriority>71</expandingIconPriority>
    <comps>
      <li Class="WorldObjectCompProperties_Timeout" />
    </comps>
  </WorldObjectDef>

</Defs>
