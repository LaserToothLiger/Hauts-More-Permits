<?xml version="1.0" encoding="utf-8" ?>
<Defs><!--these can only be contacted in comms console. They let you trade items of interest to an e-branch in exchange for branch standing-->
  
  <TraderKindDef>
    <defName>HVMP_CommerceDonations</defName>
    <label>commerce donations office</label>
    <requestable>False</requestable>
    <commonality>0</commonality>
    <category>HVMP_BranchCollector</category>
    <tradeCurrency>Favor</tradeCurrency>
    <faction>HVMP_CommerceBranch</faction>
    <hideThingsNotWillingToTrade>True</hideThingsNotWillingToTrade>
    <stockGenerators>
      <li Class="StockGenerator_BuySingleDef">
        <thingDef>Silver</thingDef>
      </li>
      <li Class="StockGenerator_BuySingleDef">
        <thingDef>Gold</thingDef>
      </li>
      <li Class="StockGenerator_BuySingleDef">
        <thingDef>ElephantTusk</thingDef>
      </li>
      <li Class="StockGenerator_BuySingleDef">
        <thingDef>ThrumboHorn</thingDef>
      </li>
    </stockGenerators>
	<modExtensions>
	  <li Class="HautsPermits.DonationTypeBeat">
		<donationString>ivory, silver, or gold</donationString>
	  </li>
	</modExtensions>
  </TraderKindDef>
