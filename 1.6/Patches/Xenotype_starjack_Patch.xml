<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Odyssey</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Biotech</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationAdd">
								<xpath>/Defs/FactionDef[@Name="HVMP_EBranchFactionBase"]/xenotypeSet/xenotypeChances</xpath>
								<value>
									<Starjack MayRequire="Ludeon.RimWorld.Biotech">0.35</Starjack>
								</value>
							</li>
						</operations>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>