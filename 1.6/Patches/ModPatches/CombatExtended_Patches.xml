<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Combat Extended</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!--QUEST MUTATOR PATCHES
					Chaincomputers with optimization issues have wider thermal explosion radius-->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="HVMP_ChainComputer"]/comps/li[@Class="HautsPermits.CompProperties_HeatPusher_OI"]/OI_haywireExplosionRadius</xpath>
					<value>
						<OI_haywireExplosionRadius>3.9</OI_haywireExplosionRadius>
					</value>
				</li>
				<!--Sites benefitting from the Static Defense mutator only have 1 added mech structure per 6.67 pawns-->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/WorldObjectDef[defName="Site"]/comps/li[@Class="HautsPermits.WorldObjectCompProperties_Mutator_GG_SD"]/SD_turretsPerPawn</xpath>
					<value>
						<SD_turretsPerPawn>0.15</SD_turretsPerPawn>
					</value>
				</li>
				<!--PERMIT PATCHES
					Combat Assist's description in the selection menu indicates its added CE-exclusive function-->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RoyalTitlePermitDef[defName="HVMP_CombatAssist"]/description</xpath>
					<value>
						<description>Link the target to an automated combat assistance program for 8 hours, improving attack accuracy, range, and resistance to suppression.</description>
					</value>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/HediffDef[defName="HVMP_CombatAssistProgram"]/stages/li/statOffsets/ShootingAccuracyPawn</xpath>
				</li>
				<!--Orbital Scalpel does 1.5x default bomb damage, and in CE's system it should have armor penetration of "Yes". Centipedes have a blunt armor rating of up to 45, so 50 is gucci-->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/HediffDef[defName="HVMP_ScalpelTracker"]/comps/li[@Class="HautsPermits.HediffCompProperties_BoomHeadshot"]/damageAmount</xpath>
					<value>
						<damageAmount>234</damageAmount>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/HediffDef[defName="HVMP_ScalpelTracker"]/comps/li[@Class="HautsPermits.HediffCompProperties_BoomHeadshot"]/armorPen</xpath>
					<value>
						<armorPen>50</armorPen>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/HediffDef[defName="HVMP_BShield"]/comps/li[@Class="HautsFramework.HediffCompProperties_DamageNegationShield"]/energyOnReset</xpath>
