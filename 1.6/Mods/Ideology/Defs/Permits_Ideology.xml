<Defs>
  
  <!--Archive level 1-->
  <RoyalTitlePermitDef ParentName="HVMP_DropResourcesBase">
    <defName>HVMP_RefineToGood</defName>
    <label>good refinement</label>
    <description>Delivers a dose of short-lived mechanites to the target non-artifact, non-artwork, non-building item, improving its quality by 1 stage. This can't bring an item above Good-quality.</description>
    <workerClass>HautsPermits.RoyalTitlePermitWorker_AlterItemQuality</workerClass>
    <minTitle>HVMP_ArchiveI</minTitle>
    <faction>HVMP_ArchiveBranch</faction>
    <permitPointCost>1</permitPointCost>
    <cooldownDays>10</cooldownDays>
    <usableOnWorldMap>false</usableOnWorldMap>
    <uiPosition>(0,0)</uiPosition>
    <royalAid>
      <favorCost>1</favorCost>
    </royalAid>
	<modExtensions>
		<li Class="HautsFramework.PermitMoreEffects">
			<invalidTargetMessage>HVMP_TargetToGood</invalidTargetMessage>
			<soundDef>NeuralSupercharger_Injection</soundDef>
			<extraNumber>0~2</extraNumber><!--can target items of Awful through Normal quality-->
			<gambaFactorRange>0</gambaFactorRange><!--chance to poof item-->
			<onUseMessage>HVMP_QualityBoost</onUseMessage>
			<forbiddenThingCategories>
			  <li>Artifacts</li>
			  <li>Books</li>
			  <li>BuildingsArt</li>
			</forbiddenThingCategories>
        </li>
	</modExtensions>
  </RoyalTitlePermitDef>
  <RoyalTitlePermitDef ParentName="HVMP_DropResourcesBase">
    <defName>HVMP_RestoreItem</defName>
    <label>restore item</label>
